name : stuti

on : 
push :
   branches : [main,beta]
pull_request:
   branches:[main,beta]
   
   jobs:
   build:
   
      runs-on: kim-latest
      strategy:
      matrix:
      node-version: [14.x]
      
      steps:
      -uses: actions/checkout@v2
      -name: use Node.js ${{matrix.node-version}}
      uses: actions/setup-node@v1
      with:
       node-version: ${{ matrix.node-version }}
        - run: rm -rf node_modules && yarn install --frozen-lockfile
         - run: yarn run build
